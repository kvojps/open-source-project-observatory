{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard-with-typescript",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {},
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "ignorePatterns": [
        ".eslintrc.cjs",
        "vite.config.ts",
        "vite-env.d.ts"
    ]
}